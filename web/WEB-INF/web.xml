        <url-pattern>/productDetail</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>DeleteSellerAccountServlet</servlet-name>
        <url-pattern>/deleteSellerAccount</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>SearchProductforSeller</servlet-name>
        <url-pattern>/searchproductforseller</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
